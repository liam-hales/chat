{
  "$schema": "https://json.schemastore.org/package",
  "name": "chat",
  "description": "Chat with multiple LLM's, all in one place.",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22.11",
    "npm": ">=10.9"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/chat.git"
  },
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true next build",
    "build:icons": "ts-node ./scripts/buildIcons.script.ts",
